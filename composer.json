
{
    "name": "syntaxerro/ymock",
    "description": "Create PHPUnit mocks with YAML format",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "M.Lacina",
            "email": "marcin.lacina@windowslive.com"
        }
    ],

    "require": {
        "php": ">=5.6.0",
        "phpunit/phpunit": "5.*",
        "symfony/yaml": "^3.3"
    },

    "autoload": {
        "psr-4": {
            "SyntaxErro\\YMock\\": "./src",
            "SyntaxErro\\Tests\\YMock\\": "./vendor"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}